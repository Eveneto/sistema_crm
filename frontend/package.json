{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/date-fns": "^2.5.3",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.10",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^19.1.7",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.8",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "firebase": "^12.2.1",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "sass": "^1.93.2",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headed": "cypress run --headed",
    "test:e2e:chrome": "cypress run --browser chrome",
    "test:e2e:firefox": "cypress run --browser firefox",
    "test:e2e:edge": "cypress run --browser edge",
    "test:e2e:all-browsers": "npm run test:e2e:chrome && npm run test:e2e:firefox",
    "test:e2e:smoke": "cypress run --spec 'cypress/e2e/auth/authentication.spec.js'",
    "test:e2e:auth": "cypress run --spec 'cypress/e2e/auth/**/*.spec.js'",
    "test:e2e:companies": "cypress run --spec 'cypress/e2e/companies*.spec.js'",
    "test:e2e:kanban": "cypress run --spec 'cypress/e2e/kanban/**/*.spec.js'",
    "test:e2e:chat": "cypress run --spec 'cypress/e2e/chat/**/*.spec.js'",
    "test:e2e:mobile": "cypress run --config viewportWidth=375,viewportHeight=812",
    "test:e2e:tablet": "cypress run --config viewportWidth=768,viewportHeight=1024",
    "test:e2e:record": "cypress run --record --key $CYPRESS_RECORD_KEY",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e:record",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react-router-dom": "^5.3.3",
    "cypress": "^15.0.0",
    "typescript": "^4.9.5",
    "webpack-dev-server": "^5.2.2"
  }
}
